        console.log('Script gestart: de pagina wordt geladen.');
        document.addEventListener('DOMContentLoaded', () => {
            console.log('DOM volledig geladen en geanalyseerd.');

            const iframes = document.querySelectorAll('iframe');
            iframes.forEach((iframe, index) => {
                iframe.addEventListener('load', () => {
                    console.log(`Iframe ${index + 1} geladen met URL: ${iframe.src}`);
                });
            });
        });
    </script>
